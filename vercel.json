{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/check-apikey",
      "dest": "/api/check-apikey.js"
    },
    {
      "src": "/check",
      "dest": "/index.html"
    }
  ]
}
